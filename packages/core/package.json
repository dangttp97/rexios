{
  "name": "@rexios/core",
  "version": "1.0.0",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    "./types": {
      "source": "./src/types/index.ts",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./client": {
      "source": "./src/client/index.ts",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./cache": {
      "source": "./src/cache/index.ts",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./persist": {
      "source": "./src/persist/index.ts",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./runtime": {
      "source": "./src/runtime/index.ts",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./pipeline": {
      "source": "./src/pipeline/index.ts",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    }
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "network",
    "rexios"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dangttp97/react-native-rexios.git"
  },
  "author": "Trần Thành Phương Đăng <phuongdang012@gmail.com> (https://github.com/dangttp97)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dangttp97/react-native-rexios/issues"
  },
  "files": [
    "src",
    "lib",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties"
  ],
  "scripts": {
    "clean": "del-cli lib",
    "prepare": "bob build",
    "typecheck": "tsc",
    "test": "jest",
    "release": "release-it --only-version",
    "lint": "eslint \"**/*.{js,ts,tsx}\""
  }
}
